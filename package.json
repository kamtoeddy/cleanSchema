{
  "author": "kamtoeddy",
  "bugs": {
    "url": "https://github.com/kamtoeddy/clean-schema/issues"
  },
  "description": "schema builder for javascript environments",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "vitest": "^0.34.5"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kamtoeddy/clean-schema#readme",
  "license": "MIT",
  "name": "clean-schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamtoeddy/clean-schema.git"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean-up": "rm -rf coverage dist",
    "lint": "eslint \"src/**/*.ts*\" && tsc --noEmit",
    "test": "npm run build && vitest run",
    "tes:cov": "npm run build && vitest run --coverage",
    "test:watch": "npm run build && vitest",
    "prepare": "npm run clean-up && npm run build"
  },
  "version": "4.0.0-beta.0",
  "keywords": [
    "schema",
    "validation"
  ]
}

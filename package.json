{
  "author": "blacksocks",
  "bugs": {
    "url": "https://github.com/kamtoeddy/clean-schema/issues"
  },
  "description": "schema builder for javascript environments",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.6.2"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/kamtoeddy/clean-schema#readme",
  "keywords": [
    "clean architecture",
    "node",
    "schema",
    "data definition layer",
    "data validation layer"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)+(test).(ts|js)"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  },
  "license": "ISC",
  "main": "lib/index.js",
  "name": "clean-schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamtoeddy/clean-schema.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "prepare": "npm run build",
    "watch": "tsc -w"
  },
  "types": "lib/index.d.ts",
  "version": "1.2.4"
}
